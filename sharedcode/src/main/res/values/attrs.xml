<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="text_font" format="enum">
        <enum name="Quicksand_Regular" value="0" />
        <enum name="Quicksand_Light" value="1" />
        <enum name="Quicksand_Medium" value="2" />
        <enum name="Quicksand_Bold" value="3" />
        <enum name="Camar" value="4" />
    </attr>

    <declare-styleable name="CustomTextView">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomCheckBox">
        <attr name="text_font" />
    </declare-styleable>

</resources>